<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="footer_links">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="sample-something-else-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Something Elses</argument>
                    <argument name="path" xsi:type="string">sample/somethingelse/index</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="sample-something-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Somethings</argument>
                <argument name="path" xsi:type="string">sample/something/index</argument>
            </arguments>
        </block>
        </referenceBlock>
    </body>
</page>
